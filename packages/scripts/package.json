{
  "name": "@land-grab-2-interactives/scripts",
  "version": "0.0.1",
  "private": true,
  "description": "Scripts for generating, storing, and deploying the data and code used by interactives in the Land Grab 2 series.",
  "scripts": {
    "arrayify-rights-type": "tsc && node dist/arrayify-rights-type.js",
    "build:interactive-map": "tsc && npm run fetch:lars && npm run arrayify-rights-type && npm run gen:tribe-parcel-links && npm run gen:university-parcel-links && npm run gen:vector-tiles",
    "build:activity-map": "tsc && npm run derive-activity-category && npm run rewind-parcels",
    "build:grid-scrollytelling": "tsc && npm run fetch:cessions && npm run fetch:territories && npm run fetch:sections && npm run fetch:townships && npm run gen:vector-tiles",
    "deploy:activity-map": "tsc && node --env-file=.env dist/deploy-activity-map.js",
    "deploy:collage": "tsc && node --env-file=.env dist/deploy-collage.js",
    "deploy:grid-scrollytelling": "tsc && node --env-file=.env dist/deploy-grid-scrollytelling.js",
    "deploy:interactive-map": "tsc && node --env-file=.env dist/deploy-interactive-map.js",
    "deploy:puf-scrollytelling": "tsc && node --env-file=.env dist/deploy-puf-scrollytelling.js",
    "deploy:tx-parcels": "tsc && node --env-file=.env dist/deploy-tx-parcels.js",
    "deploy:video-topper": "tsc && node --env-file=.env dist/deploy-video-topper.js",
    "derive-activity-category": "tsc && node dist/derive-activity-category.js",
    "fetch:cessions": "tsc && node dist/fetch-cessions.js",
    "fetch:lars": "tsc && node dist/fetch-lars.js",
    "fetch:territories": "tsc && node dist/fetch-territories.js",
    "fetch:sections": "tsc && node dist/fetch-sections.js",
    "fetch:townships": "tsc && node dist/fetch-townships.js",
    "format": "prettier --write src/**/*.ts",
    "gen:tribe-parcel-links": "tsc && node dist/generate-tribe-parcel-links.js",
    "gen:university-parcel-links": "tsc && node dist/generate-university-parcel-links.js",
    "gen:vector-tiles": "tsc && node dist/generate-vector-tiles.js",
    "rewind-parcels": "tsc && node dist/rewind-parcels.js",
    "store:fonts": "tsc && node --env-file=.env dist/store-fonts.js",
    "store:geojson": "tsc && node --env-file=.env dist/store-geojson.js",
    "store:pmtiles": "tsc && node --env-file=.env dist/store-pmtiles-archives.js",
    "store:style": "tsc && node --env-file=.env dist/store-style.js"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@land-grab-2-interactives/types": "^0.0.1",
    "@turf/turf": "^6.5.0",
    "@types/geojson": "^7946.0.13",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "lodash": "^4.17.21",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "type": "module"
}